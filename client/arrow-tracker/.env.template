# Client environment variables for Azure AD B2C authentication
# Copy this file to .env.local and fill in the values

# Azure AD B2C Configuration
REACT_APP_B2C_TENANT_NAME=your-b2c-tenant-name
REACT_APP_B2C_CLIENT_ID=your-b2c-client-id
REACT_APP_B2C_SIGN_UP_SIGN_IN_POLICY=B2C_1_signupsignin
REACT_APP_B2C_RESET_PASSWORD_POLICY=B2C_1_passwordreset
REACT_APP_B2C_EDIT_PROFILE_POLICY=B2C_1_profileedit

# Azure Function App URL (API base URL)
REACT_APP_API_BASE_URL=https://your-function-app-url

# Application Insights Instrumentation Key (optional for monitoring)
REACT_APP_APPINSIGHTS_INSTRUMENTATIONKEY=