# Environment Variables Configuration
# Copy this file to .env.local for local development
# The CI/CD pipeline will use GitHub Secrets for deployment

# API Configuration
REACT_APP_API_BASE_URL=https://api-dev.arrowtracker.com

# Authentication Configuration
REACT_APP_AZURE_CLIENT_ID=your-azure-client-id
REACT_APP_AZURE_B2C_TENANT_NAME=arrowtrackerb2c
REACT_APP_AZURE_B2C_POLICY_NAME=B2C_1_signupsignin
REACT_APP_AZURE_B2C_RESET_PASSWORD_POLICY=B2C_1_passwordreset
REACT_APP_AZURE_B2C_EDIT_PROFILE_POLICY=B2C_1_profileedit

# Feature Flags
REACT_APP_ENABLE_WEATHER_INTEGRATION=false
REACT_APP_ENABLE_ADVANCED_ANALYTICS=false